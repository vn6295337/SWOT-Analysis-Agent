# A2A Strategy Agent - Environment Configuration Example
# Copy this file to .env and fill in your API keys

# Required - Tavily API for web search and research
TAVILY_API_KEY=

# Optional - LangSmith for workflow tracing and observability
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=
LANGCHAIN_PROJECT=a2a-strategy-agent

# Optional - System configuration
LOG_LEVEL=INFO
CACHE_ENABLED=true
MAX_REVISIONS=3
QUALITY_THRESHOLD=7